{
  "name": "jekyll-static-site-builder",
  "version": "1.0.0",
  "author": "David Malone",
  "license": "MIT",
  "description": "An opinionated pipeline for creating brochure style static websites",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "build:jekyll": "jekyll build",
    "build:webpack": "webpack --mode production",
    "build": "npm run build:webpack && npm run build:jekyll",
